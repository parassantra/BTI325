<!DOCTYPE html>
<html>
    <head>
        <title> View Data </title>
    </head>
    <body>
        {{> welcome}}
        <p> array </p>
       {{#list data}}
            Name: {{name}} Age: {{age}} Occupation: {{occupation}}
       {{/list}}
        <table border = '1'>
            <tr>
                <th> Name </th>
                <th> Age </th>
                <th> Occupation </th>
                <th> Company </th>
            </tr>
        

            <!-- using arrayData -->
            {{#each data}}
                <tr>
                    <td> {{@index}}.  {{name}} </td>
                    <td> {{age}} </td>
                    <td> {{occupation}} </td>
                    <td> {{company}} </td>
                </tr>
            {{else}}
                <tr>
                    <td colspan="4"> No Data Available </td>
                </tr>
            {{/each}}
        </table>
    </body>
</html>